<%= simple_form_for meetup, remote: true do |f| %>
  <%= f.input :title, label: false, placeholder: "Meetup ID", input_html: { class: "input_field_class form-control" } %>
  <%= f.submit "Add Meetup", data: { disable_with: "submitting..." }, class: "btn btn-primary create form-control" %>
<% end %>
