<%= simple_form_for @organization, :html => { :multipart => true } do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.text_area :overview %>
  <%= f.input :employee_count %>
  <%= f.input :tech_team_size %>
  <%= f.input :website %>
  <%= f.input :twitter %>
  Logo <%= f.file_field :logo %>
  Published <%= f.check_box :published %>
  <%= f.association :tech_stacks, as: :check_boxes %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
